<div class="">
  Loading <%= @percent %>%<div class="progress progress-striped active"><div class="bar" style="width: <%= @percent %>%;"></div></div>
</div>
<div class="">
  <input type="checkbox" id="onlyshowvaliable" <%= @check == 1 "checked" : "" %>> Only show available
</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>Domain Name</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% i = 0 %>
    <% @domains.each do |url, status| %>
      <tr>
        <td><%= i += 1 %></td>
        <td><%= url %></td>
        <td><%= status %></td>
      </tr>
    <% end %>
  </tbody>
</table>


